<?xml version="1.0" encoding="utf-8"?>
<NameMakerDefs>
  <!-- Player -->
  <RulePackDef>
    <defName>NamerFactionPlayerRandomized</defName>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">name->[colonyname]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File" ListIndex="0">
          <keyword>colonyname</keyword>
          <path>Names/ColonyNames</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  <RulePackDef>
    <defName>NamerFactionBasePlayerColony</defName>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">name->Colony</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  <RulePackDef>
    <defName>NamerFactionBasePlayerTribe</defName>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">name->Village</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  <RulePackDef>
    <defName>NamerFactionBasePlayerRandomized</defName>
    <include>
      <li ListIndex="0">NamerFactionBaseOutlander</li>
    </include>
  </RulePackDef>
  <!-- Pirate -->
  <RulePackDef>
    <defName>NamerFactionPirate</defName>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">name->The [badassanimal]s</li>
        <li ListIndex="1">name->The [badassanimal] Men</li>
        <li ListIndex="2">name->The [badassanimal]s of [badassconcept]</li>
        <li ListIndex="3">name->The [badassperson]s of [badassconcept]</li>
        <li ListIndex="4">name->The [badassperson]s</li>
        <li ListIndex="5">name->The [weapon] Men</li>
        <li ListIndex="6">name->The [weapon]s</li>
        <li ListIndex="7">name->The [weapon] [badassanimal]s</li>
        <li ListIndex="8">name->The [badasscolor] [weapon]s</li>
        <li ListIndex="9">name->The [badasscolor] [badassanimal]s</li>
        <li ListIndex="10">name->The [badasscolor] [badassperson]s</li>
        <li ListIndex="11">name->The [badassperson] [groupname]</li>
        <li ListIndex="12">name->The [badassadjective] [groupname]</li>
        <li ListIndex="13">name->The [badassadjective] [badassanimal]s</li>
        <li ListIndex="14">name->The [badassadjective] [badassperson]s</li>
        <li ListIndex="15">name->[weapon] [groupname]</li>
        <li ListIndex="16">name->[badassconcept] [groupname]</li>
        <li ListIndex="17">name->[badassconcept][groupname]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File" ListIndex="0">
          <keyword>badassanimal</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File" ListIndex="1">
          <keyword>badassadjective</keyword>
          <path>Words/Adjectives/Badass</path>
        </li>
        <li Class="Rule_File" ListIndex="2">
          <keyword>badassperson</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File" ListIndex="3">
          <keyword>badassconcept</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File" ListIndex="4">
          <keyword>groupname</keyword>
          <path>Words/Nouns/GroupNames</path>
        </li>
        <li Class="Rule_File" ListIndex="5">
          <keyword>badasscolor</keyword>
          <path>Words/Nouns/Colors_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  <RulePackDef>
    <defName>NamerFactionBasePirate</defName>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">name->[badassanimal] [geography]</li>
        <li ListIndex="1">name->[badassconcept] [geography]</li>
        <li ListIndex="2">name->[badassanimal]'s [geography]</li>
        <li ListIndex="3">name->[badassperson]'s [geography]</li>
        <li ListIndex="4">geography->[terrainfeature]</li>
        <li ListIndex="5">geography->[community]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File" ListIndex="0">
          <keyword>community</keyword>
          <path>Words/Nouns/Communities</path>
        </li>
        <li Class="Rule_File" ListIndex="1">
          <keyword>badassanimal</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File" ListIndex="2">
          <keyword>badassadjective</keyword>
          <path>Words/Adjectives/Badass</path>
        </li>
        <li Class="Rule_File" ListIndex="3">
          <keyword>badassperson</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File" ListIndex="4">
          <keyword>badassconcept</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File" ListIndex="5">
          <keyword>groupname</keyword>
          <path>Words/Nouns/GroupNames</path>
        </li>
        <li Class="Rule_File" ListIndex="6">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
        <li Class="Rule_File" ListIndex="7">
          <keyword>badasscolor</keyword>
          <path>Words/Nouns/Colors_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  <!-- Outlander -->
  <RulePackDef>
    <defName>NamerFactionOutlander</defName>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">name->[subname]</li>
        <li ListIndex="1">name->[subname]</li>
        <li ListIndex="2">name->[subname]</li>
        <li ListIndex="3">name->[subname] [politicalunion]</li>
        <li ListIndex="4">name->[subname] [politicaladjective] [politicalunion]</li>
        <li ListIndex="5">name->[politicalunion] of [subname]</li>
        <li ListIndex="6">name->[politicaladjective] [politicalunion] of [subname]</li>
        <li ListIndex="7">subname->[wordname]</li>
        <li ListIndex="8">subname->[wordname]</li>
        <li ListIndex="9">subname->[wordname]</li>
        <li ListIndex="10">subname->[direction] [wordname]</li>
        <li ListIndex="11">wordname->[syl][endsyl]</li>
        <li ListIndex="12">wordname->[syl][endsyl]</li>
        <li ListIndex="13">wordname->[syl][endsyl]</li>
        <li ListIndex="14">wordname->[syl][syl][endsyl]</li>
        <li ListIndex="15">wordname->[syl][syl][endsyl]</li>
        <li ListIndex="16">wordname->[syl][syl][syl][endsyl]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File" ListIndex="0">
          <keyword>syl</keyword>
          <path>WordParts/Syllables_English</path>
        </li>
        <li Class="Rule_File" ListIndex="1">
          <keyword>endsyl</keyword>
          <path>WordParts/PlaceEndings</path>
        </li>
        <li Class="Rule_File" ListIndex="2">
          <keyword>direction</keyword>
          <path>Words/Adjectives/CompassDirections</path>
        </li>
        <li Class="Rule_File" ListIndex="3">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
        <li Class="Rule_File" ListIndex="4">
          <keyword>politicaladjective</keyword>
          <path>Words/Adjectives/PoliticalUnions</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  <RulePackDef>
    <defName>NamerFactionBaseOutlander</defName>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">name->[positiveconcept]'s [terrainfeature]</li>
        <li ListIndex="1">name->[outlander]'s [terrainfeature]</li>
        <li ListIndex="2">name->[outlander]'s [community]</li>
        <li ListIndex="3">name->[nearname]</li>
        <li ListIndex="4">name->[wordname]</li>
        <li ListIndex="5">name->[wordname]</li>
        <li ListIndex="6">nearname->[subname]-at-the-[terrainfeature]</li>
        <li ListIndex="7">nearname->[subname]-by-the-[terrainfeature]</li>
        <li ListIndex="8">subname->[wordname]</li>
        <li ListIndex="9">subname->[community]</li>
        <li ListIndex="10">wordname->[syl][syl]</li>
        <li ListIndex="11">wordname->[syl][townend]</li>
        <li ListIndex="12">wordname->[syl][syl][townend]</li>
        <li ListIndex="13">wordname->[terrainfeature][townend]</li>
        <li ListIndex="14">townend->ville</li>
        <li ListIndex="15">townend->town</li>
        <li ListIndex="16">townend->hurst</li>
        <li ListIndex="17">townend->bourne</li>
        <li ListIndex="18">townend->brad</li>
        <li ListIndex="19">townend->dale</li>
        <li ListIndex="20">townend->hope</li>
        <li ListIndex="21">townend->ing</li>
        <li ListIndex="22">townend->ness</li>
        <li ListIndex="23">townend->mere</li>
        <li ListIndex="24">townend->shaw</li>
        <li ListIndex="25">townend->wick</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File" ListIndex="0">
          <keyword>positiveconcept</keyword>
          <path>Words/Nouns/Concepts_Positive</path>
        </li>
        <li Class="Rule_File" ListIndex="1">
          <keyword>community</keyword>
          <path>Words/Nouns/Communities</path>
        </li>
        <li Class="Rule_File" ListIndex="2">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
        <li Class="Rule_File" ListIndex="3">
          <keyword>outlander</keyword>
          <pathList>
            <li ListIndex="0">Words/Nouns/People_Outlanders</li>
            <li ListIndex="1">Words/Nouns/People_Family</li>
          </pathList>
        </li>
        <li Class="Rule_File" ListIndex="4">
          <keyword>syl</keyword>
          <path>WordParts/Syllables_English</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  <!-- Tribe -->
  <RulePackDef>
    <defName>NamerFactionTribal</defName>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">name->[tribalworld] [politicalunion]</li>
        <li ListIndex="1">name->The [tribalworld] [politicalunion]</li>
        <li ListIndex="2">name->The [color] [terrainfeature] [politicalunion]</li>
        <li ListIndex="3">name->[politicalunion] of [tribalword]</li>
        <li ListIndex="4">name->The [color] [animal] [tribalword]</li>
        <li ListIndex="5">name->The [tribalword] of the [terrainfeature]</li>
        <li ListIndex="6">tribalword->[tribalwordfile]</li>
        <li ListIndex="7">tribalword->[syl][syl]</li>
        <li ListIndex="8">tribalword->[syl][syl]</li>
        <li ListIndex="9">tribalword->[syl][syl]</li>
        <li ListIndex="10">tribalword->[syl][syl][syl]</li>
        <li ListIndex="11">tribalword->[syl][syl][syl]</li>
        <li ListIndex="12">tribalword->[syl][syl][syl][syl]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File" ListIndex="0">
          <keyword>tribalwordfile</keyword>
          <path>Words/Foreign/Tribal</path>
        </li>
        <li Class="Rule_File" ListIndex="1">
          <keyword>syl</keyword>
          <path>WordParts/Syllables_Tribal</path>
        </li>
        <li Class="Rule_File" ListIndex="2">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  <RulePackDef>
    <defName>NamerFactionBaseTribal</defName>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">name->[tribalword]</li>
        <li ListIndex="1">name->[tribalword]</li>
        <li ListIndex="2">name->[color] [tribalword]</li>
        <li ListIndex="3">name->[color] [terrainfeature]</li>
        <li ListIndex="4">name->[tribalword] [terrainfeature]</li>
        <li ListIndex="5">tribalword->[tribalwordfile]</li>
        <li ListIndex="6">tribalword->[syl][syl]</li>
        <li ListIndex="7">tribalword->[syl][syl][syl]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File" ListIndex="0">
          <keyword>tribalwordfile</keyword>
          <path>Words/Foreign/Tribal</path>
        </li>
        <li Class="Rule_File" ListIndex="1">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
        <li Class="Rule_File" ListIndex="2">
          <keyword>syl</keyword>
          <path>WordParts/Syllables_Tribal</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  <!-- Special -->
  <RulePackDef>
    <defName>NamerFactionSpacer</defName>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">name->Spacer</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
</NameMakerDefs>