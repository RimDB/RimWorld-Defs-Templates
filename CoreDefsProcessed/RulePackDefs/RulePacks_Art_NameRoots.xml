      <li ListIndex="0">ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li ListIndex="0">art_name->[angstyadjective] [anyPawn_nameShort]</li>
        <li ListIndex="1">art_name->[anyPawn_nameShort]</li>
        <li ListIndex="2">art_name->[color] [anyPawn_nameShort]</li>
        <li ListIndex="3">art_name->[angstyadjective] [angstyconcept]</li>
        <li ListIndex="4">art_name->[angstyadjective] [badassconcept]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File" ListIndex="0">
          <keyword>angstyadjective</keyword>
          <path>Words/Adjectives/Angsty</path>
        </li>
        <li Class="Rule_File" ListIndex="1">
          <keyword>badassperson</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File" ListIndex="2">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
        <li Class="Rule_File" ListIndex="3">
          <keyword>angstyconcept</keyword>
          <path>Words/Nouns/Concepts_Angsty</path>
        </li>
        <li Class="Rule_File" ListIndex="4">
          <keyword>badassconcept</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  <RulePackDef>
    <defName>ArtNameUtility</defName>
    <rulePack>
      <rulesRaw>
        <li Class="Rule_File" ListIndex="0">
          <keyword>art_adjective</keyword>
          <pathList>
            <li ListIndex="0">Words/Adjectives/Badass</li>
            <li ListIndex="1">Words/Adjectives/Angsty</li>
            <li ListIndex="2">Words/Adjectives/Angsty</li>
          </pathList>
        </li>
        <li Class="Rule_File" ListIndex="1">
          <keyword>artname</keyword>
          <path>Words/Nouns/Artworks</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
