  <ThingDef ParentName="HatMakeableBase" Name="CowboyHat">
    <defName>Apparel_CowboyHat</defName>
    <label>cowboy hat</label>
    <description>Stylish yet rugged, this hat keeps the sun and rain off - and it gives anyone who wears it a bit of extra charm. It is the most important part of traditional western attire.</description>
    <statBases>
      <WorkToMake>5000</WorkToMake>
      <Mass>0.14</Mass>
      <ArmorRating_Blunt>0.03</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.03</ArmorRating_Sharp>
      <Insulation_Heat>8</Insulation_Heat>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>2</DeteriorationRate>
      <Beauty>-4</Beauty>
      <SellPriceFactor>0.70</SellPriceFactor>
    </statBases>
    <pathCost>10</pathCost>
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li ListIndex="0">Fabric</li>
      <li ListIndex="1">Leathery</li>
    </stuffCategories>
    <altitudeLayer>Item</altitudeLayer>
    <thingClass>Apparel</thingClass>
    <category>Item</category>
    <tickerType>Never</tickerType>
    <useHitPoints>True</useHitPoints>
    <comps>
      <li Class="CompProperties_Forbiddable" ListIndex="0" />
      <li ListIndex="1">
        <compClass>CompColorable</compClass>
      </li>
      <li ListIndex="2">
        <compClass>CompQuality</compClass>
      </li>
    </comps>
    <recipeMaker>
      <workSpeedStat>TailoringSpeed</workSpeedStat>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
      <workSkill>Crafting</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
      <recipeUsers>
        <li ListIndex="0">ElectricTailoringBench</li>
        <li ListIndex="1">HandTailoringBench</li>
      </recipeUsers>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/CowboyHat/CowboyHat</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
    </graphicData>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li ListIndex="0">
          <weight>10</weight>
          <only>RGBA(0.9,0.54,0.15,1)</only>
        </li>
        <li ListIndex="1">
          <weight>15</weight>
          <only>RGBA(0.9,0.6,0.18,1)</only>
        </li>
        <li ListIndex="2">
          <weight>20</weight>
          <only>RGBA(0.9,0.42,0.23,1)</only>
        </li>
      </options>
    </colorGenerator>
    <selectable>True</selectable>
    <drawGUIOverlay>true</drawGUIOverlay>
    <alwaysHaulable>True</alwaysHaulable>
    <thingCategories>
      <li ListIndex="0">Headgear</li>
    </thingCategories>
    <techLevel>Medieval</techLevel>
    <equippedStatOffsets>
      <SocialImpact>0.15</SocialImpact>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li ListIndex="0">UpperHead</li>
      </bodyPartGroups>
      <layers>
        <li ListIndex="0">Overhead</li>
      </layers>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/CowboyHat/CowboyHat</wornGraphicPath>
      <tags>
        <li ListIndex="0">Outlander</li>
      </tags>
