    <jobString>Cutting stone blocks from chunk.</jobString>
    <workAmount>1600</workAmount>
    <workSpeedStat>StonecuttingSpeed</workSpeedStat>
    <ingredients>
      <li ListIndex="0">
        <filter>
          <thingDefs>
            <li ListIndex="0">ChunkSandstone</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
