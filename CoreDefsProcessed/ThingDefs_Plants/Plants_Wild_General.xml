<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--====================================== Carpets ======================================== -->
  <ThingDef ParentName="PlantBase">
    <defName>PlantGrass</defName>
    <label>grass</label>
    <description>Wild grass. Grows anywhere there is a little light and minimally fertile ground.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Flammability>0.8</Flammability>
    </statBases>
    <pathCostIgnoreRepeat>false</pathCostIgnoreRepeat>
    <altitudeLayer>LowPlant</altitudeLayer>
    <thingClass>Plant</thingClass>
    <category>Plant</category>
    <tickerType>Long</tickerType>
    <useHitPoints>True</useHitPoints>
    <graphicData>
      <texPath>Things/Plant/Grass</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shaderType>CutoutPlant</shaderType>
    </graphicData>
    <drawerType>MapMeshOnly</drawerType>
    <hideAtSnowDepth>0.5</hideAtSnowDepth>
    <selectable>False</selectable>
    <neverMultiSelect>True</neverMultiSelect>
    <ingestible>
      <foodType>Plant</foodType>
      <nutrition>0.18</nutrition>
      <preferability>RawBad</preferability>
    </ingestible>
    <plant>
      <sowWork>260</sowWork>
      <harvestWork>40</harvestWork>
      <soundHarvesting>Harvest_Standard</soundHarvesting>
      <soundHarvestFinish>Harvest_Standard_Finish</soundHarvestFinish>
      <growDays>2.5</growDays>
      <fertilityMin>0.05</fertilityMin>
      <fertilitySensitivity>0.3</fertilitySensitivity>
      <reproduceMtbDays>3</reproduceMtbDays>
      <topWindExposure>0.4</topWindExposure>
      <maxMeshCount>9</maxMeshCount>
      <visualSizeRange>
        <min>0.4</min>
        <max>0.6</max>
      </visualSizeRange>
      <leaflessGraphicPath>Things/Plant/Grass_Leafless</leaflessGraphicPath>
    </plant>
  </ThingDef>
  <ThingDef ParentName="PlantBase">
    <defName>PlantTallGrass</defName>
    <label>tall grass</label>
    <description>Wild tall grass. Slows down anyone moving over it.</description>
    <statBases>
      <MaxHitPoints>90</MaxHitPoints>
      <Flammability>0.8</Flammability>
    </statBases>
    <pathCost>15</pathCost>
    <pathCostIgnoreRepeat>false</pathCostIgnoreRepeat>
    <altitudeLayer>LowPlant</altitudeLayer>
    <thingClass>Plant</thingClass>
    <category>Plant</category>
    <tickerType>Long</tickerType>
    <useHitPoints>True</useHitPoints>
    <graphicData>
      <texPath>Things/Plant/Grass</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shaderType>CutoutPlant</shaderType>
    </graphicData>
    <drawerType>MapMeshOnly</drawerType>
    <hideAtSnowDepth>0.5</hideAtSnowDepth>
    <selectable>False</selectable>
    <neverMultiSelect>True</neverMultiSelect>
    <ingestible>
      <foodType>Plant</foodType>
      <nutrition>0.20</nutrition>
      <preferability>RawBad</preferability>
    </ingestible>
    <plant>
      <wildClusterSizeRange>
        <min>35</min>
        <max>40</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
      <sowWork>260</sowWork>
      <harvestWork>60</harvestWork>
      <soundHarvesting>Harvest_Standard</soundHarvesting>
      <soundHarvestFinish>Harvest_Standard_Finish</soundHarvestFinish>
      <growDays>3</growDays>
      <fertilityMin>0.5</fertilityMin>
      <fertilitySensitivity>0.7</fertilitySensitivity>
      <reproduceMtbDays>3</reproduceMtbDays>
      <topWindExposure>0.4</topWindExposure>
      <maxMeshCount>9</maxMeshCount>
      <visualSizeRange>
        <min>0.7</min>
        <max>0.85</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  <!--====================================== Bushes ======================================== -->
  <ThingDef ParentName="BushBase">
    <defName>PlantBush</defName>
    <label>bush</label>
    <description>A short shrub. Does not typically talk.</description>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
      <Flammability>0.8</Flammability>
    </statBases>
    <pathCost>30</pathCost>
    <pathCostIgnoreRepeat>false</pathCostIgnoreRepeat>
    <altitudeLayer>LowPlant</altitudeLayer>
    <thingClass>Plant</thingClass>
    <category>Plant</category>
    <tickerType>Long</tickerType>
    <useHitPoints>True</useHitPoints>
    <graphicData>
      <texPath>Things/Plant/Bush</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shaderType>CutoutPlant</shaderType>
      <drawSize>1.15</drawSize>
      <shadowData>
        <volume>(0.55, 0.2, 0.2)</volume>
        <offset>(0,0,-0.25)</offset>
      </shadowData>
    </graphicData>
    <drawerType>MapMeshOnly</drawerType>
    <selectable>true</selectable>
    <neverMultiSelect>True</neverMultiSelect>
    <fillPercent>0.20</fillPercent>
    <ingestible>
      <foodType>Plant</foodType>
      <nutrition>0.30</nutrition>
      <preferability>RawBad</preferability>
    </ingestible>
    <plant>
      <wildClusterSizeRange>
        <min>2</min>
        <max>5</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
      <sowWork>260</sowWork>
      <harvestWork>190</harvestWork>
      <soundHarvesting>Harvest_Standard</soundHarvesting>
      <soundHarvestFinish>Harvest_Standard_Finish</soundHarvestFinish>
      <growDays>3</growDays>
      <fertilityMin>0.4</fertilityMin>
      <fertilitySensitivity>0.5</fertilitySensitivity>
      <topWindExposure>0.20</topWindExposure>
      <visualSizeRange>
        <min>0.7</min>
        <max>1.1</max>
      </visualSizeRange>
      <leaflessGraphicPath>Things/Plant/Bush_Leafless</leaflessGraphicPath>
    </plant>
  </ThingDef>
  <ThingDef ParentName="PlantBase">
    <defName>PlantBrambles</defName>
    <label>brambles</label>
    <description>Tangled, thorny, woody shoots covered with leaves. Slows down anyone moving over it a little bit.</description>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>0.8</Flammability>
    </statBases>
    <pathCost>15</pathCost>
    <pathCostIgnoreRepeat>false</pathCostIgnoreRepeat>
    <altitudeLayer>LowPlant</altitudeLayer>
    <thingClass>Plant</thingClass>
    <category>Plant</category>
    <tickerType>Long</tickerType>
    <useHitPoints>True</useHitPoints>
    <graphicData>
      <texPath>Things/Plant/Brambles</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shaderType>CutoutPlant</shaderType>
      <drawSize>1.7</drawSize>
    </graphicData>
    <drawerType>MapMeshOnly</drawerType>
    <selectable>False</selectable>
    <neverMultiSelect>True</neverMultiSelect>
    <ingestible>
      <foodType>Plant</foodType>
      <nutrition>0.20</nutrition>
      <preferability>RawBad</preferability>
    </ingestible>
    <plant>
      <wildClusterSizeRange>
        <min>25</min>
        <max>35</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
      <sowWork>260</sowWork>
      <harvestWork>60</harvestWork>
      <soundHarvesting>Harvest_Standard</soundHarvesting>
      <soundHarvestFinish>Harvest_Standard_Finish</soundHarvestFinish>
      <growDays>3</growDays>
      <fertilityMin>0.5</fertilityMin>
      <fertilitySensitivity>0.7</fertilitySensitivity>
      <reproduceMtbDays>3</reproduceMtbDays>
      <topWindExposure>0.15</topWindExposure>
      <maxMeshCount>4</maxMeshCount>
      <visualSizeRange>
        <min>0.7</min>
        <max>0.85</max>
      </visualSizeRange>
      <leaflessGraphicPath>Things/Plant/Brambles_Leafless</leaflessGraphicPath>
    </plant>
  </ThingDef>
  <ThingDef ParentName="PlantBase">
    <defName>PlantWildHealroot</defName>
    <label>wild healroot</label>
    <description>A slow-growing plant which yields herbal medicine when harvested.\n\nThis wild breed of healroot is difficult to cultivate. It is prized by tribespeople in colder climates.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Flammability>0.8</Flammability>
    </statBases>
    <pathCost>10</pathCost>
    <pathCostIgnoreRepeat>false</pathCostIgnoreRepeat>
    <altitudeLayer>LowPlant</altitudeLayer>
    <thingClass>Plant</thingClass>
    <category>Plant</category>
    <tickerType>Long</tickerType>
    <useHitPoints>True</useHitPoints>
    <graphicData>
      <texPath>Things/Plant/Healroot</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shaderType>CutoutPlant</shaderType>
    </graphicData>
    <drawerType>MapMeshOnly</drawerType>
    <selectable>true</selectable>
    <neverMultiSelect>True</neverMultiSelect>
    <ingestible>
      <foodType>Plant</foodType>
      <nutrition>0.15</nutrition>
      <preferability>RawBad</preferability>
    </ingestible>
    <plant>
      <wildCommonalityMaxFraction>3.0</wildCommonalityMaxFraction>
      <wildClusterSizeRange>
        <min>2</min>
        <max>5</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
      <sowWork>260</sowWork>
      <harvestWork>400</harvestWork>
      <harvestYield>1</harvestYield>
      <harvestedThingDef>HerbalMedicine</harvestedThingDef>
      <harvestTag>Standard</harvestTag>
      <soundHarvesting>Harvest_Standard</soundHarvesting>
      <soundHarvestFinish>Harvest_Standard_Finish</soundHarvestFinish>
      <growDays>6.5</growDays>
      <fertilityMin>0.5</fertilityMin>
      <fertilitySensitivity>1.0</fertilitySensitivity>
      <dieIfLeafless>false</dieIfLeafless>
      <topWindExposure>0.1</topWindExposure>
      <visualSizeRange>
        <min>0.3</min>
        <max>1.0</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
</Defs>