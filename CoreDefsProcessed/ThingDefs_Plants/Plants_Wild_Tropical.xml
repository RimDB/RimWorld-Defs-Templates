<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--============================ Carpets =============================-->
  <ThingDef ParentName="PlantBase">
    <defName>PlantShrubLow</defName>
    <label>low shrubs</label>
    <description>Low broadleaf shrubs. These grow in clusters and slow down movement somewhat.</description>
    <statBases>
      <MaxHitPoints>90</MaxHitPoints>
      <Flammability>0.8</Flammability>
    </statBases>
    <pathCost>20</pathCost>
    <pathCostIgnoreRepeat>false</pathCostIgnoreRepeat>
    <altitudeLayer>LowPlant</altitudeLayer>
    <thingClass>Plant</thingClass>
    <category>Plant</category>
    <tickerType>Long</tickerType>
    <useHitPoints>True</useHitPoints>
    <graphicData>
      <texPath>Things/Plant/ShrubLow</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shaderType>CutoutPlant</shaderType>
    </graphicData>
    <drawerType>MapMeshOnly</drawerType>
    <hideAtSnowDepth>0.5</hideAtSnowDepth>
    <selectable>False</selectable>
    <neverMultiSelect>True</neverMultiSelect>
    <ingestible>
      <foodType>Plant</foodType>
      <nutrition>0.20</nutrition>
      <preferability>RawBad</preferability>
    </ingestible>
    <plant>
      <wildClusterSizeRange>
        <min>15</min>
        <max>22</max>
      </wildClusterSizeRange>
      <wildClusterRadius>3</wildClusterRadius>
      <sowWork>260</sowWork>
      <harvestWork>60</harvestWork>
      <soundHarvesting>Harvest_Standard</soundHarvesting>
      <soundHarvestFinish>Harvest_Standard_Finish</soundHarvestFinish>
      <growDays>3</growDays>
      <fertilityMin>0.5</fertilityMin>
      <fertilitySensitivity>0.7</fertilitySensitivity>
      <reproduceMtbDays>3</reproduceMtbDays>
      <topWindExposure>0.2</topWindExposure>
      <maxMeshCount>4</maxMeshCount>
      <visualSizeRange>
        <min>0.85</min>
        <max>1.05</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
