<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <SitePartDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: string-->
    <siteTexture>string_value</siteTexture>
    <!--Type: string-->
    <expandingIconTexture>string_value</expandingIconTexture>
    <!--Type: bool-->
    <!--Default Value: false-->
    <applyFactionColorToSiteTexture>true_or_false</applyFactionColorToSiteTexture>
    <!--Type: System.Type-->
    <!--Default Value: typeof(SitePartWorker)-->
    <workerClass>class_name</workerClass>
  </SitePartDef>
</Defs>