<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <PawnKindDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: Verse.ThingDef-->
    <race>ThingDef_defName</race>
    <!--Type: RimWorld.FactionDef-->
    <defaultFactionType>FactionDef_defName</defaultFactionType>
    <!--Type: string-->
    <backstoryCategory>string_value</backstoryCategory>
    <!--Type: string-->
    <labelPlural>string_value</labelPlural>
    <!--Type: float-->
    <backstoryCryptosleepCommonality>float_value</backstoryCryptosleepCommonality>
    <!--Type: bool-->
    <!--Default Value: false-->
    <forceNormalGearQuality>true_or_false</forceNormalGearQuality>
    <!--Type: int-->
    <minGenerationAge>int_value</minGenerationAge>
    <!--Type: int-->
    <!--Default Value: 999999-->
    <maxGenerationAge>int_value</maxGenerationAge>
    <!--Type: Verse.FloatRange-->
    <!--Default Value: FloatRange.One-->
    <gearHealthRange>
      <!--Type: float-->
      <min>float_value</min>
      <!--Type: float-->
      <max>float_value</max>
    </gearHealthRange>
    <!--Type: bool-->
    <!--Default Value: false-->
    <factionLeader>true_or_false</factionLeader>
    <!--Complex List-->
    <!--Type: List<Verse.PawnKindLifeStage>-->
    <lifeStages>
      <!--Type: Verse.PawnKindLifeStage-->
      <li>
        <!--Type: string-->
        <label>string_value</label>
        <!--Type: string-->
        <labelPlural>string_value</labelPlural>
        <!--Type: string-->
        <labelMale>string_value</labelMale>
        <!--Type: string-->
        <labelMalePlural>string_value</labelMalePlural>
        <!--Type: string-->
        <labelFemale>string_value</labelFemale>
        <!--Type: string-->
        <labelFemalePlural>string_value</labelFemalePlural>
        <!--Type: Verse.GraphicData-->
        <bodyGraphicData>
          <!--Type: string-->
          <texPath>string_value</texPath>
          <!--Type: System.Type-->
          <graphicClass>class_name</graphicClass>
          <!--Type: Verse.ShaderType-->
          <!--Available Values: None, Cutout, CutoutFlying, CutoutPlant, CutoutComplex, CutoutSkin, Transparent, TransparentPostLight, MetaOverlay, Mote, MoteGlow-->
          <shaderType>enum_value</shaderType>
          <!--Type: UnityEngine.Color-->
          <!--Default Value: Color.white-->
          <color>value</color>
          <!--Type: UnityEngine.Vector2-->
          <!--Default Value: Vector2.one-->
          <drawSize>value</drawSize>
          <!--Type: Verse.ShadowData-->
          <shadowData>
            <!--Type: UnityEngine.Vector3-->
            <!--Default Value: Vector3.one-->
            <volume>value</volume>
            <!--Type: UnityEngine.Vector3-->
            <!--Default Value: Vector3.zero-->
            <offset>value</offset>
          </shadowData>
        </bodyGraphicData>
        <!--Type: Verse.GraphicData-->
        <femaleGraphicData>
          <!--Type: string-->
          <texPath>string_value</texPath>
          <!--Type: Verse.ShaderType-->
          <!--Available Values: None, Cutout, CutoutFlying, CutoutPlant, CutoutComplex, CutoutSkin, Transparent, TransparentPostLight, MetaOverlay, Mote, MoteGlow-->
          <shaderType>enum_value</shaderType>
          <!--Type: UnityEngine.Color-->
          <!--Default Value: Color.white-->
          <color>value</color>
          <!--Type: UnityEngine.Vector2-->
          <!--Default Value: Vector2.one-->
          <drawSize>value</drawSize>
          <!--Type: Verse.ShadowData-->
          <shadowData>
            <!--Type: UnityEngine.Vector3-->
            <!--Default Value: Vector3.one-->
            <volume>value</volume>
            <!--Type: UnityEngine.Vector3-->
            <!--Default Value: Vector3.zero-->
            <offset>value</offset>
          </shadowData>
        </femaleGraphicData>
        <!--Type: Verse.GraphicData-->
        <dessicatedBodyGraphicData>
          <!--Type: string-->
          <texPath>string_value</texPath>
          <!--Type: UnityEngine.Vector2-->
          <!--Default Value: Vector2.one-->
          <drawSize>value</drawSize>
        </dessicatedBodyGraphicData>
        <!--Type: Verse.BodyPartToDrop-->
        <butcherBodyPart>
          <!--Type: Verse.BodyPartGroupDef-->
          <bodyPartGroup>BodyPartGroupDef_defName</bodyPartGroup>
          <!--Type: Verse.ThingDef-->
          <thing>ThingDef_defName</thing>
          <!--Type: bool-->
          <!--Default Value: true-->
          <allowFemale>true_or_false</allowFemale>
        </butcherBodyPart>
      </li>
      <!-- ... -->
    </lifeStages>
    <!--Type: bool-->
    <!--Default Value: true-->
    <isFighter>true_or_false</isFighter>
    <!--Type: float-->
    <!--Default Value: -1.0-->
    <combatPower>float_value</combatPower>
    <!--Type: float-->
    <!--Default Value: 0.5-->
    <baseRecruitDifficulty>float_value</baseRecruitDifficulty>
    <!--Type: bool-->
    <!--Default Value: false-->
    <aiAvoidCover>true_or_false</aiAvoidCover>
    <!--Type: Verse.FloatRange-->
    <!--Default Value: FloatRange.Zero-->
    <apparelMoney>
      <!--Type: float-->
      <min>float_value</min>
      <!--Type: float-->
      <max>float_value</max>
    </apparelMoney>
    <!--Type: List<Verse.ThingDef>-->
    <apparelRequired>
      <!--Type: Verse.ThingDef-->
      <li>ThingDef_defName</li>
      <!-- ... -->
    </apparelRequired>
    <!--Type: List<string>-->
    <apparelTags>
      <!--Type: string-->
      <li>string_value</li>
      <!-- ... -->
    </apparelTags>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <apparelAllowHeadwearChance>float_value</apparelAllowHeadwearChance>
    <!--Type: bool-->
    <!--Default Value: false-->
    <apparelIgnoreSeasons>true_or_false</apparelIgnoreSeasons>
    <!--Type: Verse.FloatRange-->
    <!--Default Value: FloatRange.Zero-->
    <weaponMoney>
      <!--Type: float-->
      <min>float_value</min>
      <!--Type: float-->
      <max>float_value</max>
    </weaponMoney>
    <!--Type: List<string>-->
    <weaponTags>
      <!--Type: string-->
      <li>string_value</li>
      <!-- ... -->
    </weaponTags>
    <!--Type: Verse.FloatRange-->
    <!--Default Value: FloatRange.Zero-->
    <techHediffsMoney>
      <!--Type: float-->
      <min>float_value</min>
      <!--Type: float-->
      <max>float_value</max>
    </techHediffsMoney>
    <!--Type: List<string>-->
    <techHediffsTags>
      <!--Type: string-->
      <li>string_value</li>
      <!-- ... -->
    </techHediffsTags>
    <!--Type: float-->
    <!--Default Value: 0.1-->
    <techHediffsChance>float_value</techHediffsChance>
    <!--Type: RimWorld.QualityCategory-->
    <!--Available Values: Awful, Shoddy, Poor, Normal, Good, Superior, Excellent, Masterwork, Legendary-->
    <!--Default Value: Normal-->
    <itemQuality>enum_value</itemQuality>
    <!--Type: Verse.PawnInventoryOption-->
    <inventoryOptions>
      <!--Type: Verse.ThingDef-->
      <thingDef>ThingDef_defName</thingDef>
      <!--Type: Verse.IntRange-->
      <!--Default Value: IntRange.one-->
      <countRange>
        <!--Type: int-->
        <min>int_value</min>
        <!--Type: int-->
        <max>int_value</max>
      </countRange>
      <!--Type: float-->
      <skipChance>float_value</skipChance>
      <!--Complex List-->
      <!--Type: List<Verse.PawnInventoryOption>-->
      <subOptionsChooseOne>
        <!--Type: Verse.PawnInventoryOption-->
        <li>
          <!--Type: Verse.ThingDef-->
          <thingDef>ThingDef_defName</thingDef>
          <!--Type: Verse.IntRange-->
          <!--Default Value: IntRange.one-->
          <countRange>
            <!--Type: int-->
            <min>int_value</min>
            <!--Type: int-->
            <max>int_value</max>
          </countRange>
          <!--Type: float-->
          <!--Default Value: 1.0-->
          <choiceChance>float_value</choiceChance>
        </li>
        <!-- ... -->
      </subOptionsChooseOne>
    </inventoryOptions>
    <!--Type: float-->
    <invNutrition>float_value</invNutrition>
    <!--Type: Verse.ThingDef-->
    <invFoodDef>ThingDef_defName</invFoodDef>
    <!--Type: float-->
    <chemicalAddictionChance>float_value</chemicalAddictionChance>
    <!--Type: float-->
    <!--Default Value: 0.04-->
    <combatEnhancingDrugsChance>float_value</combatEnhancingDrugsChance>
    <!--Type: Verse.IntRange-->
    <!--Default Value: IntRange.zero-->
    <combatEnhancingDrugsCount>
      <!--Type: int-->
      <min>int_value</min>
      <!--Type: int-->
      <max>int_value</max>
    </combatEnhancingDrugsCount>
    <!--Type: bool-->
    <!--Default Value: false-->
    <trader>true_or_false</trader>
    <!--Type: string-->
    <labelMale>string_value</labelMale>
    <!--Type: string-->
    <labelMalePlural>string_value</labelMalePlural>
    <!--Type: string-->
    <labelFemale>string_value</labelFemale>
    <!--Type: string-->
    <labelFemalePlural>string_value</labelFemalePlural>
    <!--Type: bool-->
    <!--Default Value: false-->
    <wildSpawn_spawnWild>true_or_false</wildSpawn_spawnWild>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <wildSpawn_EcoSystemWeight>float_value</wildSpawn_EcoSystemWeight>
    <!--Type: Verse.IntRange-->
    <!--Default Value: IntRange.one-->
    <wildSpawn_GroupSizeRange>
      <!--Type: int-->
      <min>int_value</min>
      <!--Type: int-->
      <max>int_value</max>
    </wildSpawn_GroupSizeRange>
