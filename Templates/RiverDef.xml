<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RiverDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: int-->
    <!--Default Value: -1-->
    <spawnFlowThreshold>int_value</spawnFlowThreshold>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <spawnChance>float_value</spawnChance>
    <!--Type: int-->
    <!--Default Value: 0-->
    <degradeThreshold>int_value</degradeThreshold>
    <!--Type: RimWorld.RiverDef-->
    <!--Default Value: null-->
    <degradeChild>RiverDef_defName</degradeChild>
    <!--Complex List-->
    <!--Type: List<RimWorld.RiverDef+Branch>-->
    <branches>
      <!--Type: RimWorld.RiverDef+Branch-->
      <li>
        <!--Type: int-->
        <minFlow>int_value</minFlow>
        <!--Type: RimWorld.RiverDef-->
        <child>RiverDef_defName</child>
        <!--Type: float-->
        <chance>float_value</chance>
      </li>
      <!-- ... -->
    </branches>
    <!--Type: float-->
    <!--Default Value: 0.5-->
    <widthOnWorld>float_value</widthOnWorld>
    <!--Type: float-->
    <!--Default Value: 10.0-->
    <widthOnMap>float_value</widthOnMap>
    <!--Type: float-->
    <!--Default Value: 0.0-->
    <debugOpacity>float_value</debugOpacity>
  </RiverDef>
</Defs>