    <!--Complex List-->
    <!--Type: List<RimWorld.RoadDef+WorldRenderStep>-->
    <worldRenderSteps>
      <!--Type: RimWorld.RoadDef+WorldRenderStep-->
      <li>
        <!--Type: RimWorld.RoadWorldLayerDef-->
