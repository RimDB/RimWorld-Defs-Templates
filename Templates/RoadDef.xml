        <chancePerPositionCurve>
          <!--Simple List-->
          <!--Type: List<Verse.CurvePoint>-->
          <points>
            <!--Type: Verse.CurvePoint-->
            <li>value</li>
            <!-- ... -->
          </points>
        </chancePerPositionCurve>
      </li>
      <!--Type: RimWorld.RoadDefGenStep_DryWithFallback-->
      <li Class="RoadDefGenStep_DryWithFallback">
        <!--Type: Verse.SimpleCurve-->
        <chancePerPositionCurve>
          <!--Simple List-->
          <!--Type: List<Verse.CurvePoint>-->
          <points>
            <!--Type: Verse.CurvePoint-->
            <li>value</li>
            <!-- ... -->
          </points>
        </chancePerPositionCurve>
        <!--Type: Verse.TerrainDef-->
        <fallback>TerrainDef_defName</fallback>
      </li>
      <!--Type: RimWorld.RoadDefGenStep_Place-->
      <li Class="RoadDefGenStep_Place">
        <!--Type: Verse.SimpleCurve-->
        <chancePerPositionCurve>
          <!--Simple List-->
          <!--Type: List<Verse.CurvePoint>-->
          <points>
            <!--Type: Verse.CurvePoint-->
            <li>value</li>
            <!-- ... -->
          </points>
        </chancePerPositionCurve>
        <!--Type: float-->
        <!--Default Value: 1.0-->
        <antialiasingMultiplier>float_value</antialiasingMultiplier>
        <!--Type: int-->
        <!--Default Value: 0-->
