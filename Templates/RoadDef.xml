<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RoadDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: int-->
    <priority>int_value</priority>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <movementCostMultiplier>float_value</movementCostMultiplier>
    <!--Type: int-->
    <!--Default Value: 15-->
    <tilesPerSegment>int_value</tilesPerSegment>
    <!--Type: RimWorld.RoadPathingDef-->
    <pathingMode>RoadPathingDef_defName</pathingMode>
    <!--Complex List-->
    <!--Type: List<RimWorld.RoadDef+RoadGenStep>-->
    <roadGenSteps>
      <!--Type: RimWorld.RoadDef+RoadGenStep-->
      <li>
        <!--Type: Verse.SimpleCurve-->
        <chancePerPositionCurve>
          <!--Type: List<Verse.CurvePoint>-->
          <points>
            <!--Type: Verse.CurvePoint-->
            <li>value</li>
            <!-- ... -->
          </points>
        </chancePerPositionCurve>
        <!--Type: Verse.BuildableDef-->
        <place>BuildableDef_defName</place>
      </li>
      <!-- ... -->
    </roadGenSteps>
  </RoadDef>
</Defs>