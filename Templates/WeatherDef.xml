<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <WeatherDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: string-->
    <!--Desc: A human-readable description given when the Def is inspected by players.-->
    <description>string_value</description>
    <!--Type: Verse.IntRange-->
    <!--Default Value: new IntRange(16000, 160000)-->
    <durationRange>
      <!--Type: int-->
      <min>int_value</min>
      <!--Type: int-->
      <max>int_value</max>
    </durationRange>
    <!--Type: bool-->
    <!--Default Value: false-->
    <repeatable>true_or_false</repeatable>
    <!--Type: Verse.Favorability-->
    <!--Available Values: VeryBad, Bad, Neutral, Good, VeryGood-->
    <!--Default Value: Neutral-->
    <favorability>enum_value</favorability>
    <!--Type: Verse.FloatRange-->
    <!--Default Value: new FloatRange(-999f, 999f)-->
    <temperatureRange>
      <!--Type: float-->
      <min>float_value</min>
      <!--Type: float-->
      <max>float_value</max>
    </temperatureRange>
    <!--Type: Verse.SimpleCurve-->
    <!--Default Value: null-->
    <commonalityRainfallFactor>
      <!--Simple List-->
      <!--Type: List<Verse.CurvePoint>-->
      <points>
        <!--Type: Verse.CurvePoint-->
        <li>value</li>
        <!-- ... -->
      </points>
    </commonalityRainfallFactor>
    <!--Type: float-->
    <!--Default Value: 0.0-->
    <rainRate>float_value</rainRate>
    <!--Type: float-->
    <!--Default Value: 0.0-->
    <snowRate>float_value</snowRate>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <windSpeedFactor>float_value</windSpeedFactor>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <moveSpeedMultiplier>float_value</moveSpeedMultiplier>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <accuracyMultiplier>float_value</accuracyMultiplier>
    <!--Type: float-->
    <perceivePriority>float_value</perceivePriority>
    <!--Reference List-->
    <!--Type: List<Verse.SoundDef>-->
    <ambientSounds>
      <!--Type: Verse.SoundDef-->
      <li>SoundDef_defName</li>
      <!-- ... -->
    </ambientSounds>
    <!--Complex List-->
    <!--Type: List<Verse.WeatherEventMaker>-->
    <eventMakers>
      <!--Type: Verse.WeatherEventMaker-->
      <li>
        <!--Type: float-->
        <!--Default Value: 100.0-->
        <averageInterval>float_value</averageInterval>
        <!--Type: System.Type-->
        <!--Default Value: null-->
        <eventClass>class_name</eventClass>
      </li>
      <!-- ... -->
    </eventMakers>
    <!--Simple List-->
    <!--Type: List<System.Type>-->
    <overlayClasses>
      <!--Type: System.Type-->
      <li>class_name</li>
      <!-- ... -->
    </overlayClasses>
    <!--Type: Verse.SkyColorSet-->
    <skyColorsNightMid>
      <!--Type: UnityEngine.Color-->
      <sky>value</sky>
      <!--Type: UnityEngine.Color-->
      <shadow>value</shadow>
      <!--Type: UnityEngine.Color-->
      <overlay>value</overlay>
      <!--Type: float-->
      <saturation>float_value</saturation>
    </skyColorsNightMid>
    <!--Type: Verse.SkyColorSet-->
    <skyColorsNightEdge>
      <!--Type: UnityEngine.Color-->
      <sky>value</sky>
      <!--Type: UnityEngine.Color-->
      <shadow>value</shadow>
      <!--Type: UnityEngine.Color-->
      <overlay>value</overlay>
      <!--Type: float-->
      <saturation>float_value</saturation>
    </skyColorsNightEdge>
    <!--Type: Verse.SkyColorSet-->
    <skyColorsDay>
      <!--Type: UnityEngine.Color-->
      <sky>value</sky>
      <!--Type: UnityEngine.Color-->
      <shadow>value</shadow>
      <!--Type: UnityEngine.Color-->
      <overlay>value</overlay>
      <!--Type: float-->
      <saturation>float_value</saturation>
    </skyColorsDay>
    <!--Type: Verse.SkyColorSet-->
    <skyColorsDusk>
      <!--Type: UnityEngine.Color-->
      <sky>value</sky>
      <!--Type: UnityEngine.Color-->
      <shadow>value</shadow>
      <!--Type: UnityEngine.Color-->
      <overlay>value</overlay>
      <!--Type: float-->
      <saturation>float_value</saturation>
    </skyColorsDusk>
  </WeatherDef>
</Defs>