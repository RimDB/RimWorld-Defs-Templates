<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: string-->
    <!--Desc: A human-readable description given when the Def is inspected by players.-->
    <description>string_value</description>
    <!--Type: System.Type-->
    <!--Default Value: typeof(RecipeWorker)-->
    <workerClass>class_name</workerClass>
    <!--Type: System.Type-->
    <!--Default Value: typeof(RecipeWorkerCounter)-->
    <workerCounterClass>class_name</workerCounterClass>
    <!--Type: string-->
    <!--Default Value: "Doing an unknown recipe."-->
    <jobString>string_value</jobString>
    <!--Type: Verse.WorkTypeDef-->
    <requiredGiverWorkType>WorkTypeDef_defName</requiredGiverWorkType>
    <!--Type: float-->
    <!--Default Value: -1.0-->
    <workAmount>float_value</workAmount>
    <!--Type: RimWorld.StatDef-->
    <workSpeedStat>StatDef_defName</workSpeedStat>
    <!--Type: RimWorld.StatDef-->
    <efficiencyStat>StatDef_defName</efficiencyStat>
    <!--Complex List-->
    <!--Type: List<Verse.IngredientCount>-->
    <ingredients>
      <!--Type: Verse.IngredientCount-->
      <li>
        <!--Type: Verse.ThingFilter-->
        <!--Default Value: new ThingFilter()-->
        <filter>
          <!--Reference List-->
          <!--Type: List<Verse.ThingDef>-->
          <thingDefs>
            <!--Type: Verse.ThingDef-->
            <li>ThingDef_defName</li>
            <!-- ... -->
          </thingDefs>
          <!--Simple List-->
          <!--Type: List<string>-->
          <categories>
            <!--Type: string-->
            <li>string_value</li>
            <!-- ... -->
          </categories>
          <!--Reference List-->
          <!--Type: List<Verse.ThingDef>-->
          <exceptedThingDefs>
            <!--Type: Verse.ThingDef-->
            <li>ThingDef_defName</li>
            <!-- ... -->
          </exceptedThingDefs>
        </filter>
        <!--Type: float-->
        <!--Default Value: 1.0-->
        <count>float_value</count>
      </li>
      <!-- ... -->
    </ingredients>
    <!--Type: Verse.ThingFilter-->
    <!--Default Value: new ThingFilter()-->
    <fixedIngredientFilter>
      <!--Reference List-->
      <!--Type: List<Verse.SpecialThingFilterDef>-->
      <disallowedSpecialFilters>
        <!--Type: Verse.SpecialThingFilterDef-->
        <li>SpecialThingFilterDef_defName</li>
        <!-- ... -->
      </disallowedSpecialFilters>
      <!--Reference List-->
      <!--Type: List<Verse.ThingDef>-->
      <thingDefs>
        <!--Type: Verse.ThingDef-->
        <li>ThingDef_defName</li>
        <!-- ... -->
      </thingDefs>
      <!--Simple List-->
      <!--Type: List<string>-->
      <categories>
        <!--Type: string-->
        <li>string_value</li>
        <!-- ... -->
      </categories>
      <!--Reference List-->
      <!--Type: List<Verse.ThingDef>-->
      <exceptedThingDefs>
        <!--Type: Verse.ThingDef-->
        <li>ThingDef_defName</li>
        <!-- ... -->
      </exceptedThingDefs>
      <!--Simple List-->
      <!--Type: List<string>-->
      <exceptedCategories>
        <!--Type: string-->
        <li>string_value</li>
        <!-- ... -->
      </exceptedCategories>
      <!--Simple List-->
      <!--Type: List<string>-->
      <specialFiltersToDisallow>
        <!--Type: string-->
        <li>string_value</li>
        <!-- ... -->
      </specialFiltersToDisallow>
    </fixedIngredientFilter>
    <!--Type: Verse.ThingFilter-->
    <defaultIngredientFilter>
      <!--Reference List-->
      <!--Type: List<Verse.ThingDef>-->
      <thingDefs>
        <!--Type: Verse.ThingDef-->
        <li>ThingDef_defName</li>
        <!-- ... -->
      </thingDefs>
      <!--Simple List-->
      <!--Type: List<string>-->
      <categories>
        <!--Type: string-->
        <li>string_value</li>
        <!-- ... -->
      </categories>
      <!--Reference List-->
      <!--Type: List<Verse.ThingDef>-->
      <exceptedThingDefs>
        <!--Type: Verse.ThingDef-->
        <li>ThingDef_defName</li>
        <!-- ... -->
      </exceptedThingDefs>
      <!--Simple List-->
      <!--Type: List<string>-->
      <exceptedCategories>
        <!--Type: string-->
        <li>string_value</li>
        <!-- ... -->
      </exceptedCategories>
      <!--Simple List-->
      <!--Type: List<string>-->
      <specialFiltersToAllow>
        <!--Type: string-->
        <li>string_value</li>
        <!-- ... -->
      </specialFiltersToAllow>
    </defaultIngredientFilter>
    <!--Type: bool-->
    <!--Default Value: false-->
    <allowMixingIngredients>true_or_false</allowMixingIngredients>
    <!--Type: System.Type-->
    <!--Default Value: typeof(IngredientValueGetter_Volume)-->
    <ingredientValueGetterClass>class_name</ingredientValueGetterClass>
    <!--Reference List-->
    <!--Type: List<Verse.SpecialThingFilterDef>-->
    <forceHiddenSpecialFilters>
      <!--Type: Verse.SpecialThingFilterDef-->
      <li>SpecialThingFilterDef_defName</li>
      <!-- ... -->
    </forceHiddenSpecialFilters>
    <!--Type: List<Verse.ThingCountClass>-->
    <products>
      <!--Type: Verse.ThingCountClass-->
      <!--Tag Field: thingDef, Tag Type: Verse.ThingDef-->
      <!--Value Field: count, Value Type: int-->
      <!--Pattern: <ThingDef_defName>int_value</ThingDef_defName>-->
      <!-- ... -->
    </products>
    <!--Simple List-->
    <!--Type: List<Verse.SpecialProductType>-->
    <specialProducts>
      <!--Type: Verse.SpecialProductType-->
      <!--Available Values: Butchery, Smelted-->
      <li>enum_value</li>
      <!-- ... -->
    </specialProducts>
    <!--Type: bool-->
    <!--Default Value: false-->
    <productHasIngredientStuff>true_or_false</productHasIngredientStuff>
    <!--Type: int-->
    <!--Default Value: 1-->
    <targetCountAdjustment>int_value</targetCountAdjustment>
    <!--Type: Verse.ThingDef-->
    <unfinishedThingDef>ThingDef_defName</unfinishedThingDef>
    <!--Complex List-->
    <!--Type: List<Verse.SkillRequirement>-->
    <skillRequirements>
      <!--Type: Verse.SkillRequirement-->
      <li>
        <!--Type: RimWorld.SkillDef-->
        <skill>SkillDef_defName</skill>
        <!--Type: int-->
        <minLevel>int_value</minLevel>
      </li>
      <!-- ... -->
    </skillRequirements>
    <!--Type: RimWorld.SkillDef-->
    <workSkill>SkillDef_defName</workSkill>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <workSkillLearnFactor>float_value</workSkillLearnFactor>
    <!--Type: Verse.EffecterDef-->
    <effectWorking>EffecterDef_defName</effectWorking>
    <!--Type: Verse.SoundDef-->
    <soundWorking>SoundDef_defName</soundWorking>
    <!--Reference List-->
    <!--Type: List<Verse.ThingDef>-->
    <recipeUsers>
      <!--Type: Verse.ThingDef-->
      <li>ThingDef_defName</li>
      <!-- ... -->
    </recipeUsers>
    <!--Reference List-->
    <!--Type: List<Verse.BodyPartDef>-->
    <appliedOnFixedBodyParts>
      <!--Type: Verse.BodyPartDef-->
      <li>BodyPartDef_defName</li>
      <!-- ... -->
    </appliedOnFixedBodyParts>
    <!--Type: Verse.HediffDef-->
    <addsHediff>HediffDef_defName</addsHediff>
    <!--Type: Verse.HediffDef-->
    <removesHediff>HediffDef_defName</removesHediff>
    <!--Type: bool-->
    <!--Default Value: false-->
    <hideBodyPartNames>true_or_false</hideBodyPartNames>
    <!--Type: bool-->
    <!--Default Value: false-->
    <isViolation>true_or_false</isViolation>
    <!--Type: string-->
    <successfullyRemovedHediffMessage>string_value</successfullyRemovedHediffMessage>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <surgerySuccessChanceFactor>float_value</surgerySuccessChanceFactor>
    <!--Type: float-->
    <deathOnFailedSurgeryChance>float_value</deathOnFailedSurgeryChance>
    <!--Type: bool-->
    <!--Default Value: true-->
    <targetsBodyPart>true_or_false</targetsBodyPart>
    <!--Type: bool-->
    <!--Default Value: true-->
    <anesthetize>true_or_false</anesthetize>
    <!--Type: Verse.ResearchProjectDef-->
    <researchPrerequisite>ResearchProjectDef_defName</researchPrerequisite>
    <!--Type: RimWorld.ConceptDef-->
    <conceptLearned>ConceptDef_defName</conceptLearned>
  </RecipeDef>
</Defs>