<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <GameConditionDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: string-->
    <!--Desc: A human-readable description given when the Def is inspected by players.-->
    <description>string_value</description>
    <!--Type: System.Type-->
    <!--Default Value: typeof(GameCondition)-->
    <conditionClass>class_name</conditionClass>
    <!--Reference List-->
    <!--Type: List<Verse.GameConditionDef>-->
    <exclusiveConditions>
      <!--Type: Verse.GameConditionDef-->
      <li>GameConditionDef_defName</li>
      <!-- ... -->
    </exclusiveConditions>
    <!--Type: string-->
    <!--Default Value: (string)null-->
    <endMessage>string_value</endMessage>
    <!--Type: bool-->
    <!--Default Value: false-->
    <canBePermanent>true_or_false</canBePermanent>
    <!--Type: RimWorld.PsychicDroneLevel-->
    <!--Available Values: None, GoodMedium, BadLow, BadMedium, BadHigh, BadExtreme-->
    <!--Default Value: BadMedium-->
    <droneLevel>enum_value</droneLevel>
    <!--Type: bool-->
    <!--Default Value: false-->
    <preventRain>true_or_false</preventRain>
  </GameConditionDef>
</Defs>