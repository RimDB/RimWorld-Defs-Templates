<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <MentalStateDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: System.Type-->
    <!--Default Value: typeof(MentalState)-->
    <stateClass>class_name</stateClass>
    <!--Type: System.Type-->
    <!--Default Value: typeof(MentalStateWorker)-->
    <workerClass>class_name</workerClass>
    <!--Type: Verse.MentalStateCategory-->
    <!--Available Values: Undefined, Aggro, Sad, Panic, Indulgent, Misc-->
    <category>enum_value</category>
    <!--Type: bool-->
    <!--Default Value: true-->
    <prisonersCanDo>true_or_false</prisonersCanDo>
    <!--Type: bool-->
    <!--Default Value: false-->
    <unspawnedCanDo>true_or_false</unspawnedCanDo>
    <!--Type: bool-->
    <!--Default Value: false-->
    <colonistsOnly>true_or_false</colonistsOnly>
    <!--Type: bool-->
    <!--Default Value: false-->
    <blockNormalThoughts>true_or_false</blockNormalThoughts>
    <!--Type: Verse.EffecterDef-->
    <stateEffecter>EffecterDef_defName</stateEffecter>
    <!--Type: RimWorld.TaleDef-->
    <tale>TaleDef_defName</tale>
    <!--Type: bool-->
    <!--Default Value: false-->
    <allowBeatfire>true_or_false</allowBeatfire>
    <!--Type: RimWorld.DrugCategory-->
    <!--Available Values: None, Any, Medical, Social, Hard-->
    <!--Default Value: Any-->
    <drugCategory>enum_value</drugCategory>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <recoveryMtbDays>float_value</recoveryMtbDays>
    <!--Type: int-->
    <!--Default Value: 500-->
    <minTicksBeforeRecovery>int_value</minTicksBeforeRecovery>
    <!--Type: int-->
    <!--Default Value: 99999999-->
    <maxTicksBeforeRecovery>int_value</maxTicksBeforeRecovery>
    <!--Type: bool-->
    <!--Default Value: false-->
    <recoverFromSleep>true_or_false</recoverFromSleep>
    <!--Type: bool-->
    <!--Default Value: false-->
    <recoverFromDowned>true_or_false</recoverFromDowned>
    <!--Type: RimWorld.ThoughtDef-->
    <moodRecoveryThought>ThoughtDef_defName</moodRecoveryThought>
    <!--Type: string-->
    <beginLetter>string_value</beginLetter>
    <!--Type: string-->
    <beginLetterLabel>string_value</beginLetterLabel>
    <!--Type: Verse.LetterDef-->
    <beginLetterDef>LetterDef_defName</beginLetterDef>
    <!--Type: UnityEngine.Color-->
    <!--Default Value: Color.green-->
    <nameColor>value</nameColor>
    <!--Type: string-->
    <recoveryMessage>string_value</recoveryMessage>
    <!--Type: string-->
    <baseInspectLine>string_value</baseInspectLine>
  </MentalStateDef>
</Defs>