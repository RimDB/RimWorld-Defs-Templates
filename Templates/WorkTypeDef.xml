      <li>enum_value</li>
      <!-- ... -->
    </workTags>
    <!--Type: string-->
    <labelShort>string_value</labelShort>
    <!--Type: string-->
    <pawnLabel>string_value</pawnLabel>
    <!--Type: string-->
    <gerundLabel>string_value</gerundLabel>
    <!--Type: string-->
    <verb>string_value</verb>
    <!--Type: int-->
    <naturalPriority>int_value</naturalPriority>
    <!--Type: bool-->
    <!--Default Value: false-->
    <alwaysStartActive>true_or_false</alwaysStartActive>
