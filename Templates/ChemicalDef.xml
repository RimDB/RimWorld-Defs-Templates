<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ChemicalDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: Verse.HediffDef-->
    <addictionHediff>HediffDef_defName</addictionHediff>
    <!--Type: Verse.HediffDef-->
    <toleranceHediff>HediffDef_defName</toleranceHediff>
    <!--Type: bool-->
    <!--Default Value: true-->
    <canBinge>true_or_false</canBinge>
    <!--Type: float-->
    <onGeneratedAddictedToleranceChance>float_value</onGeneratedAddictedToleranceChance>
    <!--Complex List-->
    <!--Type: List<Verse.HediffGiver_Event>-->
    <onGeneratedAddictedEvents>
      <!--Type: Verse.HediffGiver_Event-->
      <li>
        <!--Type: Verse.HediffDef-->
        <hediff>HediffDef_defName</hediff>
        <!--Type: List<Verse.BodyPartDef>-->
        <partsToAffect>
          <!--Type: Verse.BodyPartDef-->
          <li>BodyPartDef_defName</li>
          <!-- ... -->
        </partsToAffect>
        <!--Type: float-->
        <!--Default Value: 1.0-->
        <chance>float_value</chance>
      </li>
      <!-- ... -->
    </onGeneratedAddictedEvents>
  </ChemicalDef>
</Defs>