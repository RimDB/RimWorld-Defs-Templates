      <!--Default Value: "Things/Skyfaller/SkyfallerShadowCircle"-->
      <shadow>string_value</shadow>
      <!--Type: UnityEngine.Vector2-->
      <!--Default Value: Vector2.one-->
      <shadowSize>value</shadowSize>
      <!--Type: float-->
      <cameraShake>float_value</cameraShake>
      <!--Type: bool-->
      <!--Default Value: false-->
      <rotateGraphicTowardsDirection>true_or_false</rotateGraphicTowardsDirection>
      <!--Type: Verse.SoundDef-->
      <anticipationSound>SoundDef_defName</anticipationSound>
      <!--Type: int-->
      <!--Default Value: 100-->
      <anticipationSoundTicks>int_value</anticipationSoundTicks>
