<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <LetterDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: System.Type-->
    <!--Default Value: typeof(StandardLetter)-->
    <letterClass>class_name</letterClass>
    <!--Type: UnityEngine.Color-->
    <!--Default Value: Color.white-->
    <color>value</color>
    <!--Type: UnityEngine.Color-->
    <!--Default Value: Color.white-->
    <flashColor>value</flashColor>
    <!--Type: float-->
    <!--Default Value: 90.0-->
    <flashInterval>float_value</flashInterval>
    <!--Type: bool-->
    <!--Default Value: false-->
    <bounce>true_or_false</bounce>
    <!--Type: Verse.SoundDef-->
    <arriveSound>SoundDef_defName</arriveSound>
    <!--Type: string-->
    <!--Default Value: "UI/Letters/LetterUnopened"-->
    <icon>string_value</icon>
  </LetterDef>
</Defs>