<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <IncidentDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: System.Type-->
    <workerClass>class_name</workerClass>
    <!--Type: RimWorld.IncidentCategory-->
    <!--Available Values: Undefined, Misc, ThreatSmall, ThreatBig, AllyArrival, OrbitalVisitor, ShipChunkDrop, JourneyOffer, Disease, CaravanTarget, AllyAssistance-->
    <category>enum_value</category>
    <!--Type: RimWorld.IncidentTargetType-->
    <!--Available Values: None, MapPlayerHome, MapTempIncident, MapMisc, Caravan, World, All-->
    <targetType>enum_value</targetType>
    <!--Type: float-->
    <baseChance>float_value</baseChance>
    <!--Type: RimWorld.IncidentPopulationEffect-->
    <!--Available Values: Decrease, None, Increase-->
    <populationEffect>enum_value</populationEffect>
    <!--Type: int-->
    <earliestDay>int_value</earliestDay>
    <!--Type: float-->
    <minRefireDays>float_value</minRefireDays>
    <!--Type: int-->
    <minDifficulty>int_value</minDifficulty>
    <!--Type: bool-->
    <!--Default Value: false-->
    <pointsScaleable>true_or_false</pointsScaleable>
    <!--Type: float-->
    <!--Default Value: -1.0-->
    <minThreatPoints>float_value</minThreatPoints>
    <!--Reference List-->
    <!--Type: List<RimWorld.BiomeDef>-->
    <allowedBiomes>
      <!--Type: RimWorld.BiomeDef-->
      <li>BiomeDef_defName</li>
      <!-- ... -->
    </allowedBiomes>
    <!--Simple List-->
    <!--Type: List<string>-->
    <tags>
      <!--Type: string-->
      <li>string_value</li>
      <!-- ... -->
    </tags>
    <!--Simple List-->
    <!--Type: List<string>-->
    <refireCheckTags>
      <!--Type: string-->
      <li>string_value</li>
      <!-- ... -->
    </refireCheckTags>
    <!--Type: string-->
    <letterText>string_value</letterText>
    <!--Type: string-->
    <letterLabel>string_value</letterLabel>
    <!--Type: Verse.LetterDef-->
    <letterDef>LetterDef_defName</letterDef>
    <!--Type: Verse.GameConditionDef-->
    <gameCondition>GameConditionDef_defName</gameCondition>
    <!--Type: Verse.FloatRange-->
    <durationDays>
      <!--Type: float-->
      <min>float_value</min>
      <!--Type: float-->
      <max>float_value</max>
    </durationDays>
    <!--Type: Verse.HediffDef-->
    <diseaseIncident>HediffDef_defName</diseaseIncident>
    <!--Type: Verse.FloatRange-->
    <!--Default Value: new FloatRange(0f, 0.49f)-->
    <diseaseVictimFractionRange>
      <!--Type: float-->
      <min>float_value</min>
      <!--Type: float-->
      <max>float_value</max>
    </diseaseVictimFractionRange>
    <!--Reference List-->
    <!--Type: List<Verse.BodyPartDef>-->
    <diseasePartsToAffect>
      <!--Type: Verse.BodyPartDef-->
      <li>BodyPartDef_defName</li>
      <!-- ... -->
    </diseasePartsToAffect>
    <!--Type: Verse.ThingDef-->
    <shipPart>ThingDef_defName</shipPart>
    <!--Type: List<RimWorld.MTBByBiome>-->
    <mtbDaysByBiome>
      <!--Type: RimWorld.MTBByBiome-->
      <!--Tag Field: biome, Tag Type: RimWorld.BiomeDef-->
      <!--Value Field: mtbDays, Value Type: float-->
      <!--Pattern: <BiomeDef_defName>float_value</BiomeDef_defName>-->
      <!-- ... -->
    </mtbDaysByBiome>
    <!--======== Non Matched Elements ========-->
    <minPopulation>4</minPopulation>
    <chanceFactorByPopulationCurve>
      <points>
        <li ListIndex="0">(2,0)</li>
      </points>
    </chanceFactorByPopulationCurve>
  </IncidentDef>
</Defs>