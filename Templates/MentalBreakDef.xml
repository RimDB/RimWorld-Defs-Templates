<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <MentalBreakDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: System.Type-->
    <!--Default Value: typeof(MentalBreakWorker)-->
    <workerClass>class_name</workerClass>
    <!--Type: Verse.MentalStateDef-->
    <mentalState>MentalStateDef_defName</mentalState>
    <!--Type: float-->
    <baseCommonality>float_value</baseCommonality>
    <!--Type: Verse.MentalBreakIntensity-->
    <!--Available Values: Extreme, Major, Minor-->
    <intensity>enum_value</intensity>
    <!--Type: RimWorld.TraitDef-->
    <requiredTrait>TraitDef_defName</requiredTrait>
  </MentalBreakDef>
</Defs>