    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: System.Type-->
    <!--Default Value: typeof(PawnColumnWorker)-->
    <workerClass>class_name</workerClass>
    <!--Type: bool-->
    <!--Default Value: false-->
    <sortable>true_or_false</sortable>
    <!--Type: bool-->
    <!--Default Value: false-->
    <ignoreWhenCalculatingOptimalTableSize>true_or_false</ignoreWhenCalculatingOptimalTableSize>
    <!--Type: string-->
    <headerIcon>string_value</headerIcon>
    <!--Type: string-->
    <headerTip>string_value</headerTip>
    <!--Type: int-->
    <gap>int_value</gap>
    <!--Type: int-->
    <widthPriority>int_value</widthPriority>
  </PawnColumnDef>
</Defs>