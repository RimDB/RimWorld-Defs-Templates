<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <DamageDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: System.Type-->
    <!--Default Value: typeof(DamageWorker)-->
    <workerClass>class_name</workerClass>
    <!--Type: bool-->
    <!--Default Value: false-->
    <externalViolence>true_or_false</externalViolence>
    <!--Type: bool-->
    <!--Default Value: true-->
    <hasForcefulImpact>true_or_false</hasForcefulImpact>
    <!--Type: bool-->
    <!--Default Value: true-->
    <harmsHealth>true_or_false</harmsHealth>
    <!--Type: bool-->
    <!--Default Value: true-->
    <makesBlood>true_or_false</makesBlood>
    <!--Type: bool-->
    <!--Default Value: true-->
    <canInterruptJobs>true_or_false</canInterruptJobs>
    <!--Type: string-->
    <!--Default Value: "{0} has been killed."-->
    <deathMessage>string_value</deathMessage>
    <!--Type: Verse.Sound.ImpactSoundTypeDef-->
    <impactSoundType>ImpactSoundTypeDef_defName</impactSoundType>
    <!--Type: Verse.DamageArmorCategoryDef-->
    <armorCategory>DamageArmorCategoryDef_defName</armorCategory>
    <!--Type: bool-->
    <!--Default Value: false-->
    <spreadOut>true_or_false</spreadOut>
    <!--Type: bool-->
    <!--Default Value: false-->
    <execution>true_or_false</execution>
    <!--Type: bool-->
    <!--Default Value: false-->
    <isExplosive>true_or_false</isExplosive>
    <!--Type: int-->
    <!--Default Value: 10-->
    <explosionDamage>int_value</explosionDamage>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <explosionSnowMeltAmount>float_value</explosionSnowMeltAmount>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <explosionBuildingDamageFactor>float_value</explosionBuildingDamageFactor>
    <!--Type: bool-->
    <!--Default Value: true-->
    <explosionAffectOutsidePartsOnly>true_or_false</explosionAffectOutsidePartsOnly>
    <!--Type: Verse.ThingDef-->
    <explosionCellMote>ThingDef_defName</explosionCellMote>
    <!--Type: UnityEngine.Color-->
    <!--Default Value: Color.white-->
    <explosionColorCenter>value</explosionColorCenter>
    <!--Type: UnityEngine.Color-->
    <!--Default Value: Color.white-->
    <explosionColorEdge>value</explosionColorEdge>
    <!--Type: Verse.ThingDef-->
    <explosionInteriorMote>ThingDef_defName</explosionInteriorMote>
    <!--Type: float-->
    <explosionHeatEnergyPerCell>float_value</explosionHeatEnergyPerCell>
    <!--Type: Verse.SoundDef-->
    <soundExplosion>SoundDef_defName</soundExplosion>
    <!--Type: bool-->
    <!--Default Value: false-->
    <harmAllLayersUntilOutside>true_or_false</harmAllLayersUntilOutside>
    <!--Type: bool-->
    <!--Default Value: false-->
    <hasChanceToAdditionallyDamageInnerSolidParts>true_or_false</hasChanceToAdditionallyDamageInnerSolidParts>
    <!--Type: Verse.HediffDef-->
    <hediff>HediffDef_defName</hediff>
    <!--Type: Verse.HediffDef-->
    <hediffSkin>HediffDef_defName</hediffSkin>
    <!--Type: Verse.HediffDef-->
    <hediffSolid>HediffDef_defName</hediffSolid>
    <!--Complex List-->
    <!--Type: List<Verse.DamageDefAdditionalHediff>-->
    <additionalHediffs>
      <!--Type: Verse.DamageDefAdditionalHediff-->
      <li>
        <!--Type: Verse.HediffDef-->
        <hediff>HediffDef_defName</hediff>
        <!--Type: float-->
        <!--Default Value: 0.1-->
        <severityPerDamageDealt>float_value</severityPerDamageDealt>
      </li>
      <!-- ... -->
    </additionalHediffs>
  </DamageDef>
</Defs>