    <!--Type: List<RimWorld.TrainableDef>-->
    <prerequisites>
      <!--Type: RimWorld.TrainableDef-->
      <li>TrainableDef_defName</li>
      <!-- ... -->
    </prerequisites>
    <!--Simple List-->
    <!--Type: List<string>-->
    <tags>
      <!--Type: string-->
      <li>string_value</li>
      <!-- ... -->
    </tags>
    <!--Type: bool-->
    <!--Default Value: false-->
    <defaultTrainable>true_or_false</defaultTrainable>
    <!--Type: Verse.TrainableIntelligenceDef-->
    <requiredTrainableIntelligence>TrainableIntelligenceDef_defName</requiredTrainableIntelligence>
    <!--Type: int-->
