        <guards>
          <!--Type: RimWorld.PawnGenOption-->
          <!--Tag Field: kind, Tag Type: Verse.PawnKindDef-->
          <!--Value Field: selectionWeight, Value Type: int-->
          <!--Pattern: <PawnKindDef_defName>int_value</PawnKindDef_defName>-->
          <!-- ... -->
        </guards>
      </li>
      <!-- ... -->
    </pawnGroupMakers>
    <!--Type: float-->
