<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <BiomeDef>
    <!--Type: string-->
    <!--Default Value: "UnnamedDef"-->
    <!--Desc: The name of this Def. It is used as an identifier by the game code.-->
    <defName>string_value</defName>
    <!--Type: string-->
    <!--Desc: A human-readable label used to identify this in game.-->
    <label>string_value</label>
    <!--Type: string-->
    <!--Desc: A human-readable description given when the Def is inspected by players.-->
    <description>string_value</description>
    <!--Type: System.Type-->
    <workerClass>class_name</workerClass>
    <!--Type: bool-->
    <!--Default Value: true-->
    <implemented>true_or_false</implemented>
    <!--Type: bool-->
    <!--Default Value: true-->
    <canBuildBase>true_or_false</canBuildBase>
    <!--Type: bool-->
    <!--Default Value: true-->
    <canAutoChoose>true_or_false</canAutoChoose>
    <!--Type: float-->
    <animalDensity>float_value</animalDensity>
    <!--Type: float-->
    <plantDensity>float_value</plantDensity>
    <!--Type: float-->
    <!--Default Value: 60.0-->
    <diseaseMtbDays>float_value</diseaseMtbDays>
    <!--Type: float-->
    <!--Default Value: 1.0-->
    <factionBaseSelectionWeight>float_value</factionBaseSelectionWeight>
    <!--Type: bool-->
    <!--Default Value: false-->
    <impassable>true_or_false</impassable>
    <!--Type: bool-->
    <!--Default Value: true-->
    <hasVirtualPlants>true_or_false</hasVirtualPlants>
    <!--Type: Verse.SimpleCurve-->
    <pathCost>
      <!--Type: List<Verse.CurvePoint>-->
      <points>
        <!--Type: Verse.CurvePoint-->
        <li>value</li>
        <!-- ... -->
      </points>
    </pathCost>
    <!--Type: List<RimWorld.WeatherCommonalityRecord>-->
    <baseWeatherCommonalities>
      <!--Type: RimWorld.WeatherCommonalityRecord-->
      <!--Tag Field: weather, Tag Type: Verse.WeatherDef-->
      <!--Value Field: commonality, Value Type: float-->
      <!--Pattern: <WeatherDef_defName>float_value</WeatherDef_defName>-->
      <!-- ... -->
    </baseWeatherCommonalities>
    <!--Complex List-->
    <!--Type: List<RimWorld.TerrainThreshold>-->
    <terrainsByFertility>
      <!--Type: RimWorld.TerrainThreshold-->
      <li>
        <!--Type: Verse.TerrainDef-->
        <terrain>TerrainDef_defName</terrain>
        <!--Type: float-->
        <!--Default Value: -1000.0-->
        <min>float_value</min>
        <!--Type: float-->
        <!--Default Value: 1000.0-->
        <max>float_value</max>
      </li>
      <!-- ... -->
    </terrainsByFertility>
    <!--Type: List<Verse.SoundDef>-->
    <soundsAmbient>
      <!--Type: Verse.SoundDef-->
      <li>SoundDef_defName</li>
      <!-- ... -->
    </soundsAmbient>
    <!--Complex List-->
    <!--Type: List<RimWorld.TerrainPatchMaker>-->
    <terrainPatchMakers>
      <!--Type: RimWorld.TerrainPatchMaker-->
      <li>
        <!--Complex List-->
        <!--Type: List<RimWorld.TerrainThreshold>-->
        <thresholds>
          <!--Type: RimWorld.TerrainThreshold-->
          <li>
            <!--Type: Verse.TerrainDef-->
            <terrain>TerrainDef_defName</terrain>
            <!--Type: float-->
            <!--Default Value: -1000.0-->
            <min>float_value</min>
            <!--Type: float-->
            <!--Default Value: 1000.0-->
            <max>float_value</max>
          </li>
          <!-- ... -->
        </thresholds>
        <!--Type: float-->
        <!--Default Value: 0.01-->
        <perlinFrequency>float_value</perlinFrequency>
      </li>
      <!-- ... -->
    </terrainPatchMakers>
    <!--Type: List<RimWorld.BiomePlantRecord>-->
    <wildPlants>
      <!--Type: RimWorld.BiomePlantRecord-->
      <!--Tag Field: plant, Tag Type: Verse.ThingDef-->
      <!--Value Field: commonality, Value Type: float-->
      <!--Pattern: <ThingDef_defName>float_value</ThingDef_defName>-->
      <!-- ... -->
    </wildPlants>
    <!--Type: List<RimWorld.BiomeAnimalRecord>-->
    <wildAnimals>
      <!--Type: RimWorld.BiomeAnimalRecord-->
      <!--Tag Field: animal, Tag Type: Verse.PawnKindDef-->
      <!--Value Field: commonality, Value Type: float-->
      <!--Pattern: <PawnKindDef_defName>float_value</PawnKindDef_defName>-->
      <!-- ... -->
    </wildAnimals>
    <!--Complex List-->
    <!--Type: List<RimWorld.BiomeDiseaseRecord>-->
    <diseases>
      <!--Type: RimWorld.BiomeDiseaseRecord-->
      <li>
        <!--Type: RimWorld.IncidentDef-->
        <diseaseInc>IncidentDef_defName</diseaseInc>
        <!--Type: float-->
        <!--Default Value: 999999.0-->
        <commonality>float_value</commonality>
      </li>
      <!-- ... -->
    </diseases>
    <!--Type: List<Verse.ThingDef>-->
    <allowedPackAnimals>
      <!--Type: Verse.ThingDef-->
      <li>ThingDef_defName</li>
      <!-- ... -->
    </allowedPackAnimals>
    <!--Type: bool-->
    <!--Default Value: false-->
    <hideTerrain>true_or_false</hideTerrain>
    <!--Type: string-->
    <texture>string_value</texture>
  </BiomeDef>
</Defs>